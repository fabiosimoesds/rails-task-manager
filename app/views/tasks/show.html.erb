<h1>Task</h1>

<h2><%=@task.title%></h2>
<p><%=@task.details%></p>
<p><%=@task.completed%></p>


<%= link_to "Edit Task", edit_task_path%>
<%= link_to "Back to All Tasks", tasks_path%>
<%= link_to "Delete", task_path(@task), data: {turbo_method: :delete, turbo_confirm: "Are you sure you want to DELETE this Task?"}%>
